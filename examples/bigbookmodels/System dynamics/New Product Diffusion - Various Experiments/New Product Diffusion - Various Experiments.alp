while ( chart0.getCount() >= parametersOfRuns.size() ) {
	chart0.remove( chart0.getCount() - 1 );
}
chart0.addDataSet( root.ClientsDS, "ContactRate=" + root.ContactRate, color, true, Chart.INTERPOLATION_LINEAR, 1, Chart.POINT_NONE );
while ( chart1.getCount() >= parametersOfRuns.size() ) {
	chart1.remove( chart1.getCount() - 1 );
}
chart1.addDataSet( root.SalesDS, "ContactRate=" + root.ContactRate, color, true, Chart.INTERPOLATION_LINEAR, 1, Chart.POINT_NONE );
