					<Parameter>
						<Name><![CDATA[dir]]></Name>
						<Type><![CDATA[int]]></Type>
					</Parameter>
					<Body><![CDATA[//obtain target cell and cell to walk over
int rtarget = row;
int rover = row;
int ctarget = col;
int cover = col;
switch( dir ) {
case NORTH: rtarget -= 2; rover--; break;
case SOUTH: rtarget += 2; rover++; break;
case EAST:  ctarget += 2; cover++; break;
case WEST:  ctarget -= 2; cover--; break;
}
//re-create deleted stone
Stone stone = main.add_stones();
stone.row = rover;
stone.col = cover;
stone.setXY( cover*70, rover*70 );
main.cells[rover][cover] = stone;
//unregister/register at current/target cell
main.cells[row][col] = null;
row = rtarget;
col = ctarget;
main.cells[row][col] = this;
//start moving to target cell
moveTo( col*70, row*70 );]]></Body>
