					<Parameter>
						<Name><![CDATA[r]]></Name>
						<Type><![CDATA[int]]></Type>
					</Parameter>
					<Parameter>
						<Name><![CDATA[c]]></Name>
						<Type><![CDATA[int]]></Type>
					</Parameter>
					<Body><![CDATA[if( r<0 || r>6 ) return false;
if( c<0 || c>6 ) return false;
if( r<2 && c<2 ) return false;
if( r<2 && c>4 ) return false;
if( r>4 && c<2 ) return false;
if( r>4 && c>4 ) return false;
return true;]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[void]]></ReturnType>
					<Id>1332793214470</Id>
					<Name><![CDATA[initialize]]></Name>
					<X>-200</X><Y>200</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>false</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[//clear move choices and history
moves.clear();
history.clear();
//remove all remaining stones
while( ! stones.isEmpty() )
	remove_stones( stones.get(0) );
for( int r=0; r<7; r++ ) 
	for( int c=0; c<7; c++ )
		cells[r][c] = null;
//create 32 new stones
for( int r=0; r<7; r++ ) {
	for( int c=0; c<7; c++ ) {
		if( cellExists( r, c ) ) {
			if( r==3 && c==3 )
				continue;
			Stone stone = add_stones();	
			stone.row = r;
			stone.col = c;
			stone.setXY( c*70, r*70 );
			cells[r][c] = stone;
		}	
	}
}]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[int]]></ReturnType>
					<Id>1332875903452</Id>
					<Name><![CDATA[moveToHereAvailable]]></Name>
					<X>-200</X><Y>300</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>false</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[rtarget]]></Name>
						<Type><![CDATA[int]]></Type>
					</Parameter>
					<Parameter>
						<Name><![CDATA[ctarget]]></Name>
						<Type><![CDATA[int]]></Type>
					</Parameter>
					<Body><![CDATA[for( Move mv : moves ) {
	int r = mv.row;
	int c = mv.col;
	switch( mv.dir ) {
		case Stone.NORTH: r -= 2; break;
		case Stone.SOUTH: r += 2; break;
		case Stone.EAST:  c += 2; break;
		case Stone.WEST:  c -= 2; break;
	} 
	if( r == rtarget && c == ctarget )
		return mv.dir;
}
return -1;]]></Body>
				</Function>
