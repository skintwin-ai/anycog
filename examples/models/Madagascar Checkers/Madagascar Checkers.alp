					<Name><![CDATA[canMoveAtAll]]></Name>
					<X>150</X><Y>70</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[for( int dir=NORTH; dir<=WEST; dir++ )
	if( canMove( dir ) )
		return true;
return false;]]></Body>
