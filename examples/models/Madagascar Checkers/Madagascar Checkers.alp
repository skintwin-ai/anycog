				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[boolean]]></ReturnType>
					<Id>1332790292347</Id>
					<Name><![CDATA[canMove]]></Name>
					<X>150</X><Y>50</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[dir]]></Name>
						<Type><![CDATA[int]]></Type>
					</Parameter>
					<Body><![CDATA[//obtain target cell and cell to walk over
int rtarget = row;
int rover = row;
int ctarget = col;
int cover = col;
switch( dir ) {
case NORTH: rtarget -= 2; rover--; break;
case SOUTH: rtarget += 2; rover++; break;
case EAST:  ctarget += 2; cover++; break;
case WEST:  ctarget -= 2; cover--; break;
}
if( ! main.cellExists( rtarget, ctarget ) )
	return false;
if( main.cells[rtarget][ctarget] != null )
	return false;
if( main.cells[rover][cover] == null )
	return false;
return true;]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[void]]></ReturnType>
					<Id>1332791061556</Id>
					<Name><![CDATA[move]]></Name>
					<X>150</X><Y>90</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[dir]]></Name>
						<Type><![CDATA[int]]></Type>
					</Parameter>
					<Body><![CDATA[//obtain target cell and cell to walk over
int rtarget = row;
int rover = row;
int ctarget = col;
int cover = col;
switch( dir ) {
case NORTH: rtarget -= 2; rover--; break;
case SOUTH: rtarget += 2; rover++; break;
case EAST:  ctarget += 2; cover++; break;
case WEST:  ctarget -= 2; cover--; break;
}
//delete stone walked over
main.cells[rover][cover].deleteSelf();
//unregister/register at current/target cell
main.cells[row][col] = null;
row = rtarget;
col = ctarget;
main.cells[row][col] = this;
//start moving to target cell
moveTo( col*70, row*70 );


]]></Body>
				</Function>
