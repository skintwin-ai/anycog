						<Action><![CDATA[Move lastmove = history.removeLast();
int r = lastmove.row;
int c = lastmove.col;
int dirback = 0;
switch( lastmove.dir ) {
	case Stone.NORTH: r -= 2; dirback = Stone.SOUTH; break;
	case Stone.SOUTH: r += 2; dirback = Stone.NORTH; break;
	case Stone.EAST:  c += 2; dirback = Stone.WEST;  break;
	case Stone.WEST:  c -= 2; dirback = Stone.EAST;  break;
}
Stone stone = cells[r][c];
stone.undoMove( dirback );]]></Action>
