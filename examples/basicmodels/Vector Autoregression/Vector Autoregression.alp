			<StartupCode><![CDATA[//initialize the intercepts vector
for( int i=0; i<size; i++ )
	C[i] = uniform( -1, 1 );
//initialize matrixes
for( int t=0; t<order; t++ )
	for( int i=0; i<size; i++ )
		for( int j=0; j<size; j++ )
			A[t][i][j] = uniform( -0.3, 0.3 );]]></StartupCode>
